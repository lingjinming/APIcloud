<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0" />
    <meta name="format-detection" content="telephone=no,email=no,date=no,address=no">
    <title>审核失败原因</title>
    <link rel="stylesheet" type="text/css" href="../../css/style.css" />
    <style>

    </style>
</head>

<body>
    <header id="header" class="header">
        <div class="back" tapmode onclick="api.closeWin();"></div>
        <h1 id="title">审核失败原因</h1>
    </header>
</body>
<script type="text/javascript" src="../../script/api.js"></script>
<script type="text/javascript" src="../../script/libs/zepto.js"></script>

<script type="text/javascript">
    apiready = function() {
        var header = $api.byId('header');
        $api.fixStatusBar(header);
        var headerH = $api.offset(header).h;
        var pageParam = api.pageParam;

        // 打开对应Frame
        api.openFrame({
            name: 'check_fail_frm.html',
            url: './check_fail_frm.html',
            rect: {
                marginTop: headerH,
                w: 'auto',
                h: 'auto'
            },
            pageParam: {
                pageParam:pageParam
            },
            slidBackEnabled: false,
            bounces: false,
            softInputBarEnabled: false //不显示键盘上方的工具条
        });

    };
</script>

</html>